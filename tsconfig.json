{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "outDir": "./build"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "build"]
}
